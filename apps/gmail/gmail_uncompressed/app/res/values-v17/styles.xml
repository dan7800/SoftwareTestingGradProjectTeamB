<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AccountsSwitcher.AccountDetailsStyle" parent="@style/AccountsSwitcher">
        <item name="android:textColor">@color/account_text_color</item>
        <item name="android:layout_marginStart">@dimen/start_horiz_padding</item>
    </style>
    <style name="AccountsSwitcher.AvatarStyle" parent="@style/AccountsSwitcher">
        <item name="android:layout_width">@dimen/selected_account_avatar_size</item>
        <item name="android:layout_height">@dimen/selected_account_avatar_size</item>
        <item name="android:layout_marginTop">@dimen/avatar_margin_top</item>
        <item name="android:layout_marginStart">@dimen/start_horiz_padding</item>
        <item name="android:layout_alignParentStart">true</item>
    </style>
    <style name="AccountsSwitcher.RecentsOneStyle" parent="@style/AccountsSwitcher">
        <item name="android:layout_width">@dimen/recent_account_avatar_size</item>
        <item name="android:layout_height">@dimen/recent_account_avatar_size</item>
        <item name="android:layout_marginTop">@dimen/avatar_margin_top</item>
        <item name="android:layout_marginStart">@dimen/recents_start_horiz_padding</item>
        <item name="android:layout_marginEnd">@dimen/recents_end_horiz_padding</item>
        <item name="android:layout_alignParentEnd">true</item>
    </style>
    <style name="AccountsSwitcher.RecentsTwoStyle" parent="@style/AccountsSwitcher">
        <item name="android:layout_width">@dimen/recent_account_avatar_size</item>
        <item name="android:layout_height">@dimen/recent_account_avatar_size</item>
        <item name="android:layout_marginTop">@dimen/avatar_margin_top</item>
        <item name="android:layout_toStartOf">@id/avatar_recents_one</item>
    </style>
    <style name="AccountsSwitcher.ManageAccountsTextStyle" parent="@style/AccountsSwitcher">
        <item name="android:textColor">@color/manage_accounts_text_color</item>
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:layout_marginStart">@dimen/start_horiz_padding</item>
    </style>
    <style name="AccountsSwitcher.AccountTextStyle" parent="@style/AccountsSwitcher">
        <item name="android:textColor">@color/add_accounts_text_color</item>
        <item name="android:layout_marginStart">@dimen/start_horiz_padding</item>
    </style>
    <style name="AccountsSwitcher.SelectedAccountStyle" parent="@style/AccountsSwitcher">
        <item name="android:layout_marginStart">@dimen/start_horiz_padding</item>
    </style>
    <style name="AccountsSwitcher.SelectedAccountButtonStyle" parent="@style/AccountsSwitcher">
        <item name="android:layout_width">@dimen/expander_icon_width</item>
        <item name="android:layout_alignParentEnd">true</item>
    </style>
    <style name="AccountsSwitcher.SignInTextStyle" parent="@style/AccountsSwitcher">
        <item name="android:layout_marginStart">@dimen/start_horiz_padding</item>
        <item name="android:layout_alignParentStart">true</item>
    </style>
    <style name="ChipAutocompleteWrapperStyle">
        <item name="android:paddingTop">@dimen/chip_wrapper_top_padding</item>
        <item name="android:paddingBottom">@dimen/chip_wrapper_bottom_padding</item>
        <item name="android:paddingStart">@dimen/chip_wrapper_start_padding</item>
        <item name="android:paddingEnd">@dimen/chip_wrapper_end_padding</item>
    </style>
    <style name="ChipAutocompleteDividerStyle">
        <item name="android:background">@color/autocomplete_divider_color</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">1.0px</item>
        <item name="android:layout_marginStart">@dimen/chip_wrapper_start_padding</item>
    </style>
    <style name="ChipStartIconStyle" parent="@style/ChipIconBaseStyle">
        <item name="android:layout_marginEnd">@dimen/chip_icon_margin_end</item>
    </style>
    <style name="ChipEndIconStyle" parent="@style/ChipIconBaseStyle">
        <item name="android:layout_marginStart">@dimen/chip_icon_margin_end</item>
    </style>
    <style name="ChipDeleteIconStyle" parent="@style/ChipDeleteIconBaseStyle">
        <item name="android:paddingStart">8.0dip</item>
    </style>
    <style name="RtlOverlay.Widget.AppCompat.SearchView.MagIcon" parent="@android:style/Widget">
        <item name="android:layout_marginStart">@dimen/abc_dropdownitem_text_padding_left</item>
    </style>
    <style name="RtlOverlay.Widget.AppCompat.Search.DropDown" parent="@android:style/Widget">
        <item name="android:paddingStart">@dimen/abc_dropdownitem_text_padding_left</item>
        <item name="android:paddingEnd">4.0dip</item>
    </style>
    <style name="RtlOverlay.Widget.AppCompat.Search.DropDown.Query" parent="@android:style/Widget">
        <item name="android:layout_alignParentEnd">true</item>
    </style>
    <style name="RtlOverlay.Widget.AppCompat.Search.DropDown.Icon1" parent="@android:style/Widget">
        <item name="android:layout_alignParentStart">true</item>
    </style>
    <style name="RtlOverlay.Widget.AppCompat.Search.DropDown.Icon2" parent="@android:style/Widget">
        <item name="android:layout_toStartOf">@id/edit_query</item>
    </style>
    <style name="RtlOverlay.Widget.AppCompat.Search.DropDown.Text" parent="@style/Base.Widget.AppCompat.DropDownItem.Spinner">
        <item name="android:layout_toStartOf">@android:id/icon2</item>
        <item name="android:layout_toEndOf">@android:id/icon1</item>
    </style>
    <style name="RtlOverlay.Widget.AppCompat.ActionBar.TitleItem" parent="@android:style/Widget">
        <item name="android:layout_gravity">start|center</item>
        <item name="android:paddingEnd">8.0dip</item>
    </style>
    <style name="RtlOverlay.Widget.AppCompat.ActionButton.CloseMode" parent="@style/Base.Widget.AppCompat.ActionButton.CloseMode">
        <item name="android:paddingStart">8.0dip</item>
        <item name="android:layout_marginEnd">16.0dip</item>
    </style>
    <style name="RtlOverlay.Widget.AppCompat.ActionButton.Overflow" parent="@style/Base.Widget.AppCompat.ActionButton.Overflow">
        <item name="android:paddingStart">0.0dip</item>
        <item name="android:paddingEnd">12.0dip</item>
    </style>
    <style name="RtlOverlay.Widget.AppCompat.PopupMenuItem" parent="@android:style/Widget">
        <item name="android:paddingEnd">16.0dip</item>
    </style>
    <style name="RtlOverlay.Widget.AppCompat.PopupMenuItem.InternalGroup" parent="@android:style/Widget">
        <item name="android:layout_marginStart">16.0dip</item>
    </style>
    <style name="RtlOverlay.Widget.AppCompat.PopupMenuItem.Text" parent="@android:style/Widget">
        <item name="android:textAlignment">viewStart</item>
        <item name="android:layout_alignParentStart">true</item>
    </style>
    <style name="AdSenderImageStyle">
        <item name="android:layout_marginStart">@dimen/ad_header_sender_image_margin_start</item>
    </style>
    <style name="AdSenderNameStyle" parent="@style/MessageSenderNameStyle">
        <item name="android:layout_marginStart">@dimen/message_header_inner_side_padding</item>
    </style>
    <style name="AdVisibleUrlStyle" parent="@style/MessageHeaderSubtitleStyle">
        <item name="android:layout_marginStart">@dimen/message_header_inner_side_padding</item>
    </style>
    <style name="AdBadgeStyle" parent="@style/ConversationSubjectStyle">
        <item name="android:paddingEnd">@dimen/ad_header_inner_padding</item>
    </style>
    <style name="AdInfoStyle">
        <item name="android:paddingStart">@dimen/ad_header_inner_padding</item>
        <item name="android:paddingEnd">@dimen/ad_header_inner_padding</item>
    </style>
    <style name="LabelItemIcon" parent="@style/AbstractLabelItemIcon">
        <item name="android:layout_alignParentStart">true</item>
    </style>
    <style name="FixPermissionsDialogTitle">
        <item name="android:paddingStart">@dimen/fix_permissions_dialog_title_margin_start</item>
    </style>
    <style name="FixPermissionsIcon">
        <item name="android:layout_marginStart">@dimen/fix_permissions_dialog_icon_margin_start</item>
    </style>
    <style name="FixPermissionsIndentedText">
        <item name="android:paddingStart">@dimen/fix_permissions_indented_text_margin_start</item>
        <item name="android:paddingEnd">@dimen/fix_permissions_indented_text_margin_end</item>
    </style>
    <style name="FixPermissionsSpinner">
        <item name="android:layout_marginStart">@dimen/fix_permissions_spinner_margin_start</item>
        <item name="android:layout_marginEnd">@dimen/fix_permissions_spinner_margin_end</item>
    </style>
    <style name="FixPermissionsRadioButton">
        <item name="android:paddingEnd">@dimen/fix_permissions_radio_button_padding_end</item>
        <item name="android:layout_marginStart">@dimen/fix_permissions_radio_button_margin_start</item>
    </style>
    <style name="AccountListUnreadCount" parent="@style/UnreadCount">
        <item name="android:layout_marginEnd">@dimen/account_unread_count_margin_end</item>
    </style>
</resources>
